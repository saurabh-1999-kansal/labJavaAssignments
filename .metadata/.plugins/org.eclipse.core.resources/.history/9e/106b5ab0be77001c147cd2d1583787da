package com.day1.q1;

import java.math.BigDecimal;

public class q1 {
    
	public static void main(String[] args) {

		int f1=1;
		int f2=1;
		
		System.out.print(f1+ " ");
		System.out.print(f2+ " ");
		
		int sum=f1+f2;
		for(int i=3;i<=20;i++) {
			int f3=f1+f2;
			sum+=f3;
			System.out.print(f3+ " ");
			f1=f2;
			f2=f3;	
		}
		System.out.println();
		double average=sum/20;
		System.out.println(average);
	}
        
}
